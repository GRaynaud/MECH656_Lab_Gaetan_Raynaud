@article{sotoudeh_formation_2019,
	title = {Formation of {Rotating} {Vortex} {Rope} in the {Francis}-99 {Draft} {Tube}},
	volume = {240},
	issn = {1755-1315},
	url = {https://doi.org/10.1088%2F1755-1315%2F240%2F2%2F022017},
	doi = {10.1088/1755-1315/240/2/022017},
	abstract = {The aim of this research is to understand the mechanism(s) of RVR formation during the changes in operating condition from the Best Efficiency Point (BEP) to Part Load (PL). A Computational Fluid Dynamic (CFD) methodology by the means of ANSYS-CFX is applied on a reduced high head Francis turbine model. The reduced model consists of one stay vane, two guide vanes, one runner blade, one splitter and a full draft tube. Numerical simulation is first performed at BEP as well as PL to ensure the appropriate employment of turbulence models and boundary conditions. In the second step, the inlet boundary conditions are changed linearly from BEP to PL in order to achieve the transient conditions inside the draft tube. The initial condition of the second step is the converged BEP result. The transient simulation is continued until the RVR is fully developed in the draft tube at part load condition. The numerical results for BEP, PL and BEP to PL are in a good agreement with the experimental data. The effect of the RVR is considered from two aspects. The first one is the frequency, and the amplitude of the pressure pulsations induced by the RVR in the draft tube. The second one is the velocity field in the draft tube which is investigated over time during load rejection. Moreover, the flow structure is visualized using the λ2 criterion. The mechanism(s) of RVR formation and damping is accurately investigated by the presented approach. Furthermore, the results provide a better understanding of the physics behind the RVR formation. The obtained results aim to design an effective RVR controlling approach.},
	language = {en},
	urldate = {2019-09-04},
	journal = {IOP Conference Series: Earth and Environmental Science},
	author = {Sotoudeh, N. and Maddahian, R. and Cervantes, M. J.},
	month = mar,
	year = {2019},
	keywords = {\_tablet},
	pages = {022017}
}

@article{trivedi_fluid-structure_2017,
	title = {Fluid-structure interactions in {Francis} turbines: {A} perspective review},
	volume = {68},
	issn = {1364-0321},
	shorttitle = {Fluid-structure interactions in {Francis} turbines},
	url = {http://www.sciencedirect.com/science/article/pii/S1364032116306335},
	doi = {10.1016/j.rser.2016.09.121},
	abstract = {Competitive electricity prices and reduced profit margins have forced hydraulic turbines to operate under critical conditions. The demand for extended operating ranges and the high efficiency of the turbine runners have forced manufacturers to produce lightweight runners. A turbine runner sometimes experiences resonance when a forced (flow-induced) excitation frequency approaches the runner’s natural frequency, resulting in failure. The cost of structural failure after commissioning is prohibitive. To attain a reliable and safe runner design, understanding of the structural response to flow-induced excitations is important. High amplitude pressure pulsations cause fatigue loading of the blades, which develop cracks over time. The amplitudes are dependent on the flow conditions, type of turbine and stator/rotor vane combinations. The structural response is dependent on the material properties, flow-induced damping and natural frequencies. Moreover, in a hydraulic turbine, changes in flow velocity from less than 1ms−1 to over 40ms−1 create challenges in predicting the response. The main objective of this article is to review the studies conducted on fluid-structure interactions within hydraulic turbines. Several aspects are reviewed, such as flow-induced excitation, added mass effect, hydrodynamic damping, and blade flutter. Both experimental and numerical studies are discussed in this article. This review also discusses the consequences of an increased number of transient cycles, such as load variation, start-stop and total load rejection, on the turbines and the fatigue loading. Finally, an attempt is made to highlight the important requirements for prospective fluid-structure analysis to fill current gaps in the literature.},
	urldate = {2019-05-10},
	journal = {Renewable and Sustainable Energy Reviews},
	author = {Trivedi, Chirag and Cervantes, Michel J.},
	month = feb,
	year = {2017},
	keywords = {Design, Fatigue, Flow, Fluid-structure interaction, Francis turbine, Numerical modeling, Pressure, Vibration, \_tablet},
	pages = {87--101}
}

@article{xiao_dynamic_2008,
	title = {Dynamic stresses in a francis turbine runner based on fluid-structure interaction analysis},
	volume = {13},
	issn = {1007-0214},
	doi = {10.1016/S1007-0214(08)70096-8},
	abstract = {Fatigue and cracks have occurred in many large hydraulic turbines after they were put into production. The cracks are thought to be due to dynamic stresses in the runner caused by hydraulic forces. Computational fluid dynamics (CFD) simulations that included the spiral case, stay vane, guide vane, runner vane, and draft tube were run at various operating points to analyze the pressure distribution on the runner surface and the stress characteristics in the runner due to the fluid-structure interactions (FSI). The dynamic stresses in the Francis turbine runner at the most dangerous operating point were then analyzed. The results show that the dynamic stresses caused by the hydraulic forces during off-design operating points are one of the main reasons for the fatigue and cracks in the runner blade. The results can be used to optimize the runner and to analyze other critical components in the hydraulic turbine.},
	number = {5},
	journal = {Tsinghua Science and Technology},
	author = {Xiao, R. and Wang, Z. and Luo, Y.},
	month = oct,
	year = {2008},
	keywords = {Blades, Computational modeling, Dynamics, Electron tubes, Fluids, Francis turbine, Hydraulic turbines, Stress, \_tablet, computational fluid dynamics (CFD), fluid-structure interaction (FSI), stress characteristics},
	pages = {587--592}
}

