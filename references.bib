@article{m._griffin_vortex-excited_1973,
	title = {The vortex-excited resonant vibrations of circular cylinders},
	volume = {31},
	doi = {10.1016/S0022-460X(73)80377-3},
	abstract = {It is a well-known natural phenomenon that the alternate vortex shedding that accompanies the flow past a bluff obstacle can excite the body into resonant, transverse vibrations when the vortex shedding frequency is sufficiently near to one of the body frequencies. Measurements have been made of the response characteristics and flow fields about circular cylinders that were mounted on springs in a wind tunnel. Free stream Reynolds numbers for two of the cylindrical models ranged respectively from 350 to 550 and from 550 to 900. The measured vibration frequencies, amplitudes of vibration, and phase angles between oscillatory lift and motion are compared with predictions made with an heuristic wake oscillator model for the vortex-excited vibrations. Steady drag measurements were made and show that the drag coefficient increases by as much as 75\% at the maximum vibration amplitude from the measured stationary cylinder value. A series of flow visualization photographs were taken during a portion of the shedding cycle and the observed flow field is compared with fluid velocity and cylinder motion measurements recorded simultaneously near the model. Lift amplification and energy transfer from the flow during resonance are compared over the range of the experiments. The photographs give qualitative evidence to support the physical model recently proposed by Gerrard for the vortex formation region and the transfer of circulation in the base region of a bluff body.},
	journal = {Journal of Sound and Vibration - J SOUND VIB},
	author = {M. Griffin, O and A. Skop, R and Koopmann, Gary},
	month = dec,
	year = {1973}
}

